{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": "Content type voor alle pagina's op de website"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "metaDescription": {
      "type": "text"
    },
    "heroSection": {
      "type": "component",
      "repeatable": false,
      "component": "sections.hero"
    },
    "contentBlocks": {
      "type": "dynamiczone",
      "components": [
        "blocks.text",
        "blocks.image",
        "blocks.cta",
        "blocks.testimonial"
      ]
    },
    "seoMetadata": {
      "type": "component",
      "repeatable": false,
      "component": "metadata.seo"
    }
  }
}
